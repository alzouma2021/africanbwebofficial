    <div class=\"col-lg-6\"><span>Prénom: {{ user.prenoms }}</span></div>      \n                            </div>\n                            <div class=\"row\">\n                                <div class=\"col-lg-6\"><span>E-mail: {{ user.email }}</span></div>\n                                <div class=\"col-lg-6\"><span>Pseudo: {{ user.login }}</span></div>      \n                            </div>\n                        </v-card-text>\n                            \n                    </v-card><br><br>\n\n                    <v-card elevation=\"\">\n                        <v-card-text>\n                            <div class=\"row\">\n                                <div class=\"col-lg-6\"><span class=\"font-weight-bold\">Role Utilisateur</span></div>\n                                <div class=\"col-lg-6\"><span>{{ user.roleCode }}</span></div>      \n                            </div>\n                        </v-card-text>\n                    </v-card>\n                </v-tab-item>\n                <v-tab-item key=\"Opérations\">\n                    <v-card elevation=\"\">\n                        <v-card-text>\n                            <div class=\"row\">\n                                <div class=\"col-lg-6\"><span class=\"font-weight-bold\">Status</span></div>\n                                <div class=\"col-lg-6\">\n                                    <v-chip x-small color=\"success\">Active</v-chip>\n                                </div>      \n                            </div>\n                        </v-card-text>\n                    </v-card><br>\n\n                    <v-card elevation=\"\">\n                        <v-card-text>\n                            <div class=\"row\">\n                                <div class=\"col-lg-6\"><span class=\"font-weight-bold\">Activé</span></div>\n                                <div class=\"col-lg-6\">\n                                    <span><v-switch v-model=\"switch1\" inset></v-switch></span>\n                                </div>      \n                            </div>\n                        </v-card-text>\n                    </v-card><br>\n\n                    <v-card elevation=\"\">\n                        <v-card-text>\n                            <div class=\"row\">\n                                <div class=\"col-lg-6\"><span class=\"font-weight-bold\">Permissions</span></div>\n                                <div class=\"col-lg-6\"><span>Active ou pas</span></div>      \n                            </div>\n                        </v-card-text>\n                    </v-card>\n                    \n                </v-tab-item>\n\n            </v-tabs-items>\n        </v-container>\n    </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport $ from \"jquery\"\nimport { API_GET_ALL_USERS_CREATED, HEADERS } from \"@/components/globalConfig/globalConstConfig\"\nexport default {\n    data(){\n        return{\n\n            user: {},\n            simpleObjectToSend:{\n                data:{}\n            },\n\n            switch1: true,\n            tab: null,\n            items: [\"Vue d'ensemble\", \"Données\", \"Opérations\"],\n            text: 'Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.',\n        }\n    },\n\n    methods:{\n\n        // RECUPERER LA LISTE DES UTILISATEURS CREES SUR LA PLATEFORME\n        async toGetAllUsersCreated(){\n            await axios.post(API_GET_ALL_USERS_CREATED, this.simpleObjectToSend, { headers : HEADERS(this.$store.state.userAuthentified.token) }).then((response) => {\n                if (response.status == 200) {\n                    if (response.data.status.code != 800) {\n                        this.errorMsg = response.data.status.message\n                        $(\".alert-error\").fadeIn();\n                        setTimeout(function(){\n                            $(\".alert-error\").fadeOut(); \n                        }, 4000)\n                    }else{\n                        response.data.items.forEach(element => {\n                            if (element.nom  == this.$route.params.nom) {\n                                this.user = element; \n                            }\n                        });\n                    }\n                }else{\n                    this.errorMsg = \"Erreur\";\n                    $(\".alert-error\").fadeIn();\n                    setTimeout(function(){\n                        $(\".alert-error\").fadeOut(); \n                    }, 4000)\n                }\n            }).catch((e) => {\n                this.errorMsg = e\n            }).finally(() => {\n                console.log(\"Finally\")\n            })\n        }\n\n    },\n\n    mounted(){\n        this.toGetAllUsersCreated();\n    }\n}\n</script>\n\n"]},"metadata":{},"sourceType":"module"}